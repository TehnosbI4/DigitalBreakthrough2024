@model List<AnalyzeResult>

<table class="table table-hover justify-content-center align-items-center">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Аудио</th>
        <th scope="col"></th>
        <th score="col"></th>
    </tr>
    </thead>
    <tbody>
    @if (Model is not null)
    {
        for (var i = 0; i < Model.Count; i++)
        {
            var row = Model[i];
            
            <tr>
                <th scope="row">@(i + 1)</th>
                <td>
                    <audio controls src="@row.Name"></audio>
                </td>
                <td>
                    @if (row.Status == "0")
                    {
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-check" viewBox="0 0 16 16">
                            <path d="M10.97 4.97a.75.75 0 0 1 1.07 1.05l-3.99 4.99a.75.75 0 0 1-1.08.02L4.324 8.384a.75.75 0 1 1 1.06-1.06l2.094 2.093 3.473-4.425z"/>
                        </svg>
                    }
                    else
                    {
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="white" class="bi bi-x" viewBox="0 0 16 16">
                            <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708"/>
                        </svg>  
                    }
                </td>
                <td>
                    <div>
                        @row.Text + "лалалалалалаллалалалалаллалалалиkfffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff"
                    </div>
                </td>
            </tr>
        }
    }
    </tbody>
</table>
